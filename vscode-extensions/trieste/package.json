{
  "name": "trieste-syntax",
  "displayName": "Trieste Syntax Highlighting",
  "description": "Syntax highlighting for Trieste files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "trieste",
        "aliases": ["Trieste", "trieste"],
        "extensions": [".trieste"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "trieste",
        "scopeName": "source.trieste",
        "path": "./syntaxes/trieste.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Trieste Syntax Colors",
        "uiTheme": "vs-dark",
        "path": "./themes/trieste-colors.json"
      }
    ]
  }
}