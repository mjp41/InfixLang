# Example of binding

module Module1
  module Module3
    function (x: Int) inc : Int = x inc # Loop to avoid definition
  ## After resolution this will be unchanged.
  type Foo = Module3

module Test
  # This brings inc into scope.
  ## After resolution this should be:
  ##   use ..::Module1::Module3
  use Module1::Foo

  function test (x: Int): Int =
    x inc